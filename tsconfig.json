{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2020",
    "outDir": "./dist",
    "rootDir": "./src",
    "importHelpers": false,
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
